{
  "link": {
    "shortcut icon": "{base}/htdocs/img/logo.png"
  },
  "layout": "default",
  "layoutError": "error",
  "dev": {
    "css": {
      "default": {
        "main": "{tema}/htdocs/css/dist/main.css"
      }
    },
    "js": {
      "default": {
        "jquery": "{base}/node_modules/jquery/dist/jquery.min.js",
        "bootstrap": "{base}/node_modules/bootstrap/dist/js/bootstrap.min.js"
      }
    }
  },
  "prod": {
    "css": {
      "default": {
        "main": "{tema}/htdocs/css/styles.min.css"
      }
    },
    "js": {
      "default": {
        "main": "{tema}/htdocs/js/jd.bundle.js"
      }
    }
  }
}